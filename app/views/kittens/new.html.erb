<h1>Create a Kitten</h1>

<%= form_for(@kitten) do |f| %>
	<%= render 'shared/error_messages' %>

	<%= f.label :name %>
	<%= f.text_field :name %>

	<%= f.label :age %>
	<%= f.text_field :age %>

	<%= f.label :cuteness %>
	<%= f.text_field :cuteness %>

	<%= f.label :softness %>
	<%= f.text_field :softness %>

	<%= f.submit "Create" %>

<% end %>

